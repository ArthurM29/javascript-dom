{
  "manifest_version": 2,
  "name": "String Generator",
  "description": "Generate random strings of specified length to use in testing.",
  "version": "1.0",
  "icons": {
    "128": "icons/main-icon.png"
  },
  "browser_action": {
    "default_icon": "icons/main-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}

